{
  "name": "selgetabel",
  "version": "0.1.3",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:api": "pnpm --filter @selgetabel/api dev",
    "dev:docker": "docker compose -f docker/docker-compose.dev.yml up -d",
    "docker:build": "docker compose -f docker/docker-compose.build.yml build",
    "docker:up": "docker compose -f docker/docker-compose.build.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.build.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.build.yml logs -f",
    "docker:prod": "cd docker && docker compose up -d",
    "docker:prod:down": "cd docker && docker compose down",
    "docker:prod:logs": "cd docker && docker compose logs -f",
    "docker:prod:pull": "cd docker && docker compose pull",
    "docker:init-data": "cd docker && ./scripts/init-data.sh --all",
    "docker:backup": "cd docker && ./scripts/backup.sh",
    "docker:upgrade": "cd docker && ./scripts/upgrade.sh",
    "release": "cd docker && ./build-push.sh",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.22.0",
    "pnpm": "10.28.0"
  }
}